{
	"manifest_version": 2,
	"name": "__MSG_extName__",
	"default_locale": "zh_CN",
	"description": "__MSG_extDesp__",
	"version": "1.0.6",
	"content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'",
	"background": {
		"scripts": ["res/background.js"] 
	},
	"page_action": {
		"default_icon": {
			"19": "res/pt_256.png",
			"38": "res/pt_256.png"
		},
		"default_title": "UDN社区提醒您，该网站是莆田系医院网址，请谨慎就医！",
		"default_popup": "res/popup.html"
	},
	"content_scripts": [{
		"all_frames": false,
		"css": [],
		"js": [ "res/content.js" ],
		"matches": [ "http://*/*", "https://*/*" ]
	}],
	"icons": {
		"128": "res/pt_256.png",
		"64": "res/pt_256.png",
		"32": "res/pt_256.png",
		"16": "res/pt_256.png"
	},
	"permissions" : ["tabs"]
}
